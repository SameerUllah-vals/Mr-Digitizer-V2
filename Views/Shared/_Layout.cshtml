    <!-- END CORE TEMPLATE JS - END -->
    <script src="~/assets/js/preloader.js"></script>
    <script src="~/assets/js/jquery.validate.min.js"></script>
    <script src="~/assets/js/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/assets/js/jquery.unobtrusive-ajax.js"></script>
    <script src="~/assets/js/main.js"></script>
    <script src="~/js/paging.min.js"></script>
    <script type="text/javascript">
        $(document).on("change", "#city_filter", function () {
            showServiceProviders('');
        });
        $(document).on("click", "#btnSearch", function () {
            showServiceProviders('');
        });
        $(document).on("change", "input[name='displayType']", function () {
            if ($(this).attr('data-page-name') == 'article') {
                showServiceProviders('article');
            }
            else {
                showServiceProviders('');
            }
                
        });
        function showServiceProviders(pageName) {
            hideError();
            $.ajax({
                type: "POST",
                url: "/dashboard/getmarkers",
                data: pageName == '' ? "_keyword=" + $("#keyword").val() + "&_CityId=" + $("#city_filter").val() : "_keyword=" + $("#keyword").val(),
                success: function (response) {
                    console.log(response);
                    if (response.Success == true) {
                            destroyList();
                            let isMap = $('#Map').is(':checked');
                        if (isMap) {
                            var markers = response.Data;
                            if (pageName === 'article') {
                                 navigator.geolocation.getCurrentPosition((position) => {
                                     const p = position.coords;
                                     let mapOptions = {
                                         center: new google.maps.LatLng(p.latitude, p.longitude),
                                         zoom: 8,
                                         mapTypeId: google.maps.MapTypeId.ROADMAP
                                     };

                                     var marker, i;
                                     var map = new google.maps.Map(document.getElementById("map"), mapOptions);
                                     for (i = 0; i < markers.length; i++) {
                                         var record = markers[i];
                                         console.log(record);
                                         var marker_data = record.LatitudeLongitude.split(",");
                                         var myLatlng = new google.maps.LatLng(marker_data[0], marker_data[1]);
                                         var marker = new google.maps.Marker({
                                             position: myLatlng,
                                             map: map,
                                             title: record.Title
                                         });
                                         var content_html = "<div>";
                                         content_html += "<div class='map-info-title'>" + record.Title + "</div>";
                                         content_html += "<div class='map-info-content' style='font-size: 10px !important;line-height: 15px !important;'>";
                                         content_html += "<strong style='font-size: 10px !important;line-height: 15px !important;'>Address :</strong> " + record.Address + "<br/>";
                                         content_html += "<strong style='font-size: 10px !important;line-height: 15px !important;'>Email Address :</strong> " + record.EmailAddress + "<br/>";
                                         content_html += "<strong style='font-size: 10px !important;line-height: 15px !important;'>Phone No :</strong> " + record.PhoneNumber + "<br/>";
                                         content_html += "<strong style='font-size: 10px !important;line-height: 15px !important;'>Website :</strong> <a style='font-size: 10px !important;line-height: 15px !important;' target='_blank' href='" + record.WebsiteUrl + "'>Visit</a><br/><br/>";
                                         content_html += "<a style='font-size: 10px !important;line-height: 15px !important;' href='https://www.google.com/maps/dir/Current+Location/" + record.LatitudeLongitude + "'>View Direction</a>";
                                         content_html += "</div>";
                                         content_html += "</div>";
                                         var infowindow = new google.maps.InfoWindow();
                                         google.maps.event.addListener(marker, 'click', (function (marker, content_html, infowindow) {
                                             return function () {
                                                 infowindow.setContent(content_html);
                                                 infowindow.open(map, marker);
                                             };
                                         })(marker, content_html, infowindow));
                                     }
                                })
                            }
                            else {
                                var location = response.FieldName.split(',');
                                let mapOptions = {
                                    center: new google.maps.LatLng(location[0], location[1]),
                                    zoom: 8,
                                    mapTypeId: google.maps.MapTypeId.ROADMAP
                                };

                                var marker, i;
                                var map = new google.maps.Map(document.getElementById("map"), mapOptions);
                                for (i = 0; i < markers.length; i++) {
                                    var record = markers[i];
                                    console.log(record);
                                    var marker_data = record.LatitudeLongitude.split(",");
                                    var myLatlng = new google.maps.LatLng(marker_data[0], marker_data[1]);
                                    var marker = new google.maps.Marker({
                                        position: myLatlng,
                                        map: map,
                                        title: record.Title
                                    });
                                    var content_html = "<div>";
                                    content_html += "<div class='map-info-title'>" + record.Title + "</div>";
                                    content_html += "<div class='map-info-content' style='font-size: 10px !important;line-height: 15px !important;'>";
                                    content_html += "<strong style='font-size: 10px !important;line-height: 15px !important;'>Address :</strong> " + record.Address + "<br/>";
                                    content_html += "<strong style='font-size: 10px !important;line-height: 15px !important;'>Email Address :</strong> " + record.EmailAddress + "<br/>";
                                    content_html += "<strong style='font-size: 10px !important;line-height: 15px !important;'>Phone No :</strong> " + record.PhoneNumber + "<br/>";
                                    content_html += "<strong style='font-size: 10px !important;line-height: 15px !important;'>Website :</strong> <a style='font-size: 10px !important;line-height: 15px !important;' target='_blank' href='" + record.WebsiteUrl + "'>Visit</a><br/><br/>";
                                    content_html += "<a style='font-size: 10px !important;line-height: 15px !important;' href='https://www.google.com/maps/dir/Current+Location/" + record.LatitudeLongitude + "'>View Direction</a>";
                                    content_html += "</div>";
                                    content_html += "</div>";
                                    var infowindow = new google.maps.InfoWindow();
                                    google.maps.event.addListener(marker, 'click', (function (marker, content_html, infowindow) {
                                        return function () {
                                            infowindow.setContent(content_html);
                                            infowindow.open(map, marker);
                                        };
                                    })(marker, content_html, infowindow));
                                }
                            }
                               
                            }
                        else {

                            for (record of response.Data) {
                                $("#record-table tbody").append(`
                                        <tr><td> ${record.Title} <br> <i>${record.CityName}</i></td>  <td> ${record.Group}</td> <td> ${record.PostalCode}</td>  <td><a class="waves-effect waves-light btn btn-sm modal-trigger " href="${'@ViewBag.WebsiteURL'}dashboard/ServiceProviderDetails?FacilityId=${record.Id}">View Details</a></td></tr>`);
                            }
                            createList();

                        }
                    }
                    else {

                        showError();
                    }

                },
                error: function (data) {
                    console.log(data);
                }
            });

        }
        function createList() {
            $("#map").hide();
            $('#record-table').show();
            $('#record-table').dataTable({
                "bPaginate": false,
                "bLengthChange": false,
                "bFilter": false,
                "bInfo": false,
                "bAutoWidth": false
            });
        }
        function destroyList() {
            $('#record-table').DataTable().destroy();
            $('#record-table tbody').empty();
            $('#record-table').hide();
            $('#map').show();
        }
        function showError() {
            $('#record-table tbody').empty();
            $('#record-table').hide();
            $('#map').empty();
            $('#map').hide();
            $('.error').show();
        }
        function hideError() {
            $('.error').hide();
        }
    </script>
    @RenderSection("scripts", false)
</body>

</html>