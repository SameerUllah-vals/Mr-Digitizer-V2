            <div class="sidebar-left-secondary rtl-ps-none" data-perfect-scrollbar="" data-suppress-scroll-x="true">
                <!-- Submenu Dashboards-->

                @if (ViewBag.UserRolePermissionRecords != null)
                {
                    List<BackendMenus> ParentMenuRecords = (List<BackendMenus>)ViewBag.UserRoleParentPermissionRecords;
                    ParentMenuRecords = ParentMenuRecords.Where(x => x.Type == "SuperAdmin" && x.Status == EnumStatus.Enable).ToList();
                    List<RoleBackendMenus> RolePremissionRecords = (List<RoleBackendMenus>)ViewBag.UserRolePermissionRecords;
                    foreach (var ParentMenuRecord in ParentMenuRecords)
                    {

                        var ChildMenuRecords = RolePremissionRecords.Where(o => o.Menu.ParentId == ParentMenuRecord.Id && o.Menu.Status == EnumStatus.Enable && o.Menu.Type == "SuperAdmin").OrderBy(o => o.Position).ToList();
                        if (ChildMenuRecords.Count > 0)
                        {
                            <ul class="childNav" data-parent="di-@ParentMenuRecord.Id">
                                @foreach (var ChildMenuRecord in ChildMenuRecords)
                                {
                                    string _menuUrl = ViewBag.WebsiteURL + ChildMenuRecord.Menu.AccessUrl;
                                    <li class="nav-item"><a href="@_menuUrl"><i class="nav-icon @ChildMenuRecord.Menu.IconClass"></i><span class="item-name">@ChildMenuRecord.Menu.Title</span></a></li>
                                }
                            </ul>

                        }

                    }


                }

